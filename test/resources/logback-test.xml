<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.helpers.NOPAppender"/>

    <logger name="application" level="INFO">
        <appender-ref ref="STDOUT"/>
    </logger>

    <logger name="akka" level="ERROR">
        <appender-ref ref="STDOUT"/>
    </logger>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
